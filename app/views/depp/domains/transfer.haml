= render 'depp/shared/title', name: t(:transfer_domain)

.row
  .col-md-12
    .panel.panel-default
      .panel-heading
        %h3.panel-title= t('result')
      .panel-body
        %dl.dl-horizontal
          %dt= t('domain_name')
          %dd= @data.css('name').text

          - @data.css('trnData').children.each do |x|
            - next if x.blank?
            %dt= t(x.name)
            %dd= x.text
